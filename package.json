{
    "name": "start-markup-sass",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "build:prod": "rimraf dist/* && cross-env NODE_ENV=production webpack",
        "build": "rimraf dist/* && cross-env NODE_ENV=development webpack",
        "dev": "cross-env START_SERVER=wds NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --port 3000",
        "start": "cross-env START_SERVER=browsersync npm run build -- --watch",
        "start:prod": "cross-env START_SERVER=browsersync npm run build:prod -- --watch",
        "server": "browser-sync start --server dist --files dist --port 3000",
        "precommit": "lint-staged",
        "eslint": "eslint .",
        "ghpages": "gh-pages -d dist"
    },
    "lint-staged": {
        "*.{js, jsx}": [
            "eslint --max-warnings=0",
            "prettier --write",
            "git add"
        ]
    },
    "license": "MIT",
    "dependencies": {
        "custom-event-polyfill": "^1.0.6",
        "intersection-observer": "^0.5.0",
        "whatwg-fetch": "^3.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.1.6",
        "@babel/preset-env": "^7.1.6",
        "@babel/register": "^7.0.0",
        "autoprefixer": "^9.1.3",
        "babel-loader": "^8.0.4",
        "browser-sync": "^2.24.6",
        "browser-sync-webpack-plugin": "^2.2.2",
        "compression": "^1.7.3",
        "copy-webpack-plugin": "^4.5.2",
        "cross-env": "^5.2.0",
        "css-loader": "^1.0.0",
        "cssnano": "^4.0.5",
        "eslint": "^5.5.0",
        "eslint-config-prettier": "^3.0.1",
        "eslint-loader": "^2.1.0",
        "eslint-plugin-prettier": "^3.0.0",
        "file-loader": "^2.0.0",
        "gh-pages": "^2.0.1",
        "html-beautify-webpack-plugin": "^1.0.5",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^1.2.0",
        "lint-staged": "^8.1.0",
        "mini-css-extract-plugin": "^0.4.1",
        "node-sass": "^4.9.3",
        "postcss-loader": "^3.0.0",
        "postcss-preset-env": "^6.4.0",
        "prettier": "^1.14.2",
        "pug": "^2.0.3",
        "pug-loader": "^2.4.0",
        "raw-loader": "^0.5.1",
        "rimraf": "^2.6.2",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "stylelint": "^9.5.0",
        "stylelint-config-standard": "^18.2.0",
        "svg-sprite-loader": "^4.1.3",
        "svgo": "^1.0.5",
        "svgo-loader": "^2.1.0",
        "webpack": "^4.16.5",
        "webpack-bundle-analyzer": "^3.0.3",
        "webpack-cli": "^3.1.0",
        "webpack-dev-server": "^3.1.5",
        "webpack-merge": "^4.1.4",
        "webpack-spritesmith": "^0.5.2"
    },
    "repository": "https://github.com/fivemru/start-markup",
    "author": "fivemru (https://github.com/fivemru/)"
}
